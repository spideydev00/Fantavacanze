<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Disabilita la durata minima del frame per Core Animation su iPhone, utile per animazioni più fluide -->
    <key>CADisableMinimumFrameDurationOnPhone</key>
    <true/>

    <!-- Lingua di sviluppo predefinita per le risorse localizzate (sostituita da build setting) -->
    <key>CFBundleDevelopmentRegion</key>
    <string>it</string>

    <!-- Nome visualizzato sotto l'icona dell'app sulla Home -->
    <key>CFBundleDisplayName</key>
    <string>Fantavacanze</string>

    <!-- Nome eseguibile dell'app (definito dal build system) -->
    <key>CFBundleExecutable</key>
    <string>$(EXECUTABLE_NAME)</string>

    <!-- Lingue supportate dall'app -->
    <key>CFBundleLocalizations</key>
    <array>
      <string>it</string>
      <string>en</string>
    </array>

    <!-- Identificatore univoco del bundle (es. com.miodominio.app) -->
    <key>CFBundleIdentifier</key>
    <string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>

    <!-- Versione del formato Info.plist (sempre 6.0 per le versioni recenti) -->
    <key>CFBundleInfoDictionaryVersion</key>
    <string>6.0</string>

    <!-- Nome breve dell'app, usato internamente -->
    <key>CFBundleName</key>
    <string>Fantavacanze</string>

    <!-- Tipo di pacchetto (APPL indica un'applicazione) -->
    <key>CFBundlePackageType</key>
    <string>APPL</string>

    <!-- Signature legacy a 4 caratteri (spesso non usata, placeholder) -->
    <key>CFBundleSignature</key>
    <string>????</string>

    <!-- URL Schemes per deep linking e integrazioni (Facebook, Google, Supabase, ecc.) -->
    <key>CFBundleURLTypes</key>
    <array>
        <!-- Integrazione Facebook Login -->
        <dict>
            <key>CFBundleURLSchemes</key>
            <array>
                <string>YOUR_FACEBOOK_APP_ID_HERE</string>
            </array>
        </dict>
        <!-- Integrazione Google Sign-In e Supabase custom domain -->
        <dict>
            <key>CFBundleTypeRole</key>
            <string>Editor</string>
            <key>CFBundleURLSchemes</key>
            <array>
                <string>YOUR_GOOGLE_CLIENT_ID_HERE</string>
                <string>YOUR_SUPABASE_CUSTOM_DOMAIN_HERE</string>
            </array>
        </dict>
        <!-- Deep linking per il client Flutter Supabase -->
        <dict>
            <key>CFBundleTypeRole</key>
            <string>Editor</string>
            <key>CFBundleURLSchemes</key>
            <array>
                <string>YOUR_SUPABASE_DEEPLINK_SCHEME_HERE</string>
            </array>
        </dict>
    </array>

    <!-- Versione di marketing (es. 1.2.3), fornita da Flutter build -->
    <key>CFBundleShortVersionString</key>     
    <string>$(FLUTTER_BUILD_NAME)</string> 
    <!-- Numero interno di build (incrementale) -->
    <key>CFBundleVersion</key> 
    <string>$(FLUTTER_BUILD_NUMBER)</string>

    <!-- Configurazioni Facebook SDK -->
    <key>FacebookAppID</key>
    <string>YOUR_FACEBOOK_APP_ID_HERE</string>
    <key>FacebookClientToken</key>
    <string>YOUR_FACEBOOK_CLIENT_TOKEN_HERE</string>
    <key>FacebookDisplayName</key>
    <string>Fantavacanze</string>

    <!-- Identificatore AdMob per Google Ads -->
    <key>GADApplicationIdentifier</key>
    <string>YOUR_ADMOB_APP_ID_HERE</string>

    <!-- Schemi URL consentiti per canOpenURL (es. per WebView o integrazioni) -->
    <key>LSApplicationQueriesSchemes</key>
    <array>
        <string>https</string>
    </array>

    <!-- Richiede iOS come sistema operativo minimo -->
    <key>LSRequiresIPhoneOS</key>
    <true/>

    <!-- Descrizioni per richieste di permessi (fotocamera, microfono, libreria foto) -->
    <key>NSCameraUsageDescription</key>
    <string>L'app richiede l'accesso alla fotocamera per permetterti di scattare foto da salvare come ricordi.</string>
    <key>NSMicrophoneUsageDescription</key>
    <string>L'app richiede l'accesso al microfono per permetterti di registrare video con audio.</string>
    <key>NSPhotoLibraryUsageDescription</key>
    <string>L'app richiede l'accesso alla galleria fotografica per permetterti di selezionare immagini da salvare come ricordi.</string>

    <!-- Supporto per eventi di input indiretti (es. Apple TV) -->
    <key>UIApplicationSupportsIndirectInputEvents</key>
    <true/>

    <!-- Messaggio per richiesta di tracciamento (IDFA) -->
    <key>NSUserTrackingUsageDescription</key>
    <string>Questi dati ci aiutano a mostrarti annunci più pertinenti e a migliorare la tua esperienza.</string>

